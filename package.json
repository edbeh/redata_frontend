{
  "name": "redata_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bugsnag/js": "^7.20.0",
    "@bugsnag/plugin-react": "^7.19.0",
    "@bugsnag/source-maps": "^2.3.1",
    "@headlessui/react": "^1.7.2",
    "@hookform/resolvers": "^2.9.8",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "dayjs": "^1.11.5",
    "framer-motion": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-loading-skeleton": "^3.1.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.8",
    "react-virtual": "^2.10.4",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$(git rev-parse --short HEAD) REACT_APP_TIMESTAMP=$(git log -1 --pretty='%ad' --date=format:'%Y%m%d') react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build && REACT_APP_VERSION=$(git rev-parse --short HEAD) REACT_APP_TIMESTAMP=$(git log -1 --pretty='%ad' --date=format:'%Y%m%d') npm run bugsnag",
    "bugsnag": "bugsnag-source-maps upload-browser --api-key $REACT_APP_BUGSNAG_API_KEY --app-version \"$REACT_APP_TIMESTAMP-$REACT_APP_VERSION\" --base-url $REACT_APP_URL --directory build/static/js --overwrite",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen": "orval --config ./orval.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.49",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8"
  }
}
